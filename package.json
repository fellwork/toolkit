{
  "name": "@fellwork/toolkit",
  "version": "0.1.1",
  "packageManager": "pnpm@7.0.0-rc.3",
  "description": "Fellwork tools",
  "keywords": [],
  "homepage": "https://github.com/fellwork/toolkit#readme",
  "bugs": {
    "url": "https://github.com/fellwork/toolkit/issues"
  },
  "license": "MIT",
  "author": "Shane McGuirt <shane@fellwork.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fellwork/toolkit.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@fellwork/eslint-config": "latest",
    "@rushstack/eslint-patch": "^1.1.3",
    "@antfu/ni": "^0.14.0",
    "@babel/types": "^7.17.0",
    "@types/node": "^17.0.25",
    "bumpp": "^7.1.1",
    "esbuild": "^0.14.36",
    "eslint": "^8.13.0",
    "esno": "^0.14.1",
    "pnpm": "^7.0.0-rc.3",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.2",
    "vite": "^2.9.1",
    "vitest": "^0.9.3"
  },
  "engines": {
    "pnpm": ">=7.0.0-rc.3"
  }
}
